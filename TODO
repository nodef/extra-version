version.next('3.3.3', '1.0.0');
// '4.0.0'

version.next('3.3.3', '1');
// '4.0.0'

version.next('3.3.3', '0.1');
// '3.4.0'

version.next('3.3.3', '0.0.1');
// '3.3.4'

version.next('3.3.3-alpha', '0.0.1');
// '3.3.4-alpha'

version.next('3.3.3-alpha.3.3', '0.0.0-0.0.1');
// '3.3.3-alpha.3.4'

version.next('3.3.3-alpha.3.3', '-..1');
// '3.3.3-alpha.3.4'

version.next('3.3.3-alpha.3.3', '1');
// '4.0.0-alpha.0.0'
